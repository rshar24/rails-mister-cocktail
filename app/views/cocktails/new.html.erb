<div class="container">
  <h1>New Cocktail</h1>
  <%= simple_form_for (@cocktail) do |f| %>
    <div class="form-inputs">
      <%= f.input :name %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
    </div>
    <div class="form-actions">
      <%= f.button :submit, class: "btn btn-primary" %>
    </div>
  <% end %>
</div>
